<table>
    <tr>
        <th>ID</th>
        <th>Email</th>
        <th>Tên</th>
        <th>Số điện thoại</th>
        <th>Ngày sinh</th>
        <th>Địa chỉ</th>
        <th>Giới tính</th>
        <th>Hành động</th>
    </tr>
    <% for(user of users) {%>
        <tr>
            <td><%= user.id %></td>
            <td><%= user.email %></td>
            <td><%= user.fullName %></td>
            <td><%= user.phone %></td>
            <td><%= user.birthday %></td>
            <td><%= user.address %></td>
            <td><% if(user.gender) {%>Nam<% }else {%>Nữ<% } %></td>
            <td>Xoá</td>
        </tr>
        <% } %>  
</table>